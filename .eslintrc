{
  "root": true,
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "arrow-parens": 0,
    "global-require": 0,
    "react/jsx-filename-extension": 0,
    "for-direction": 0,
    "getter-return": 0,
    "no-buffer-constructor": 0,
    "function-paren-newline": 0,
    "semi-style": 0,
    "switch-colon-spacing": 0,
    "import/no-extraneous-dependencies": 0,
    "no-mixed-operators": 0,
    "object-curly-newline": 0,
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
  },
}
